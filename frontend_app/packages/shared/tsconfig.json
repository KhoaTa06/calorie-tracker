{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": ["../../node_modules/*", "/src/*"],
      "axios": ["../../node_modules/axios/index"],
    },
  },
  "include": ["src/**/*"],
  "exclude": ["dist"]
}